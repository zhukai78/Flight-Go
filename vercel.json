{
  "version": 2,
  "routes": [
    {
      "src": "^/assets/(.*)",
      "dest": "/build/web/assets/$1"
    },
    {
      "src": "^/flutter_service_worker.js",
      "dest": "/build/web/flutter_service_worker.js",
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {
      "src": "^/main.dart.js",
      "dest": "/build/web/main.dart.js"
    },
    {
      "src": "^/favicon.png",
      "dest": "/build/web/favicon.png"
    },
    {
      "src": "^/manifest.json",
      "dest": "/build/web/manifest.json"
    },
    {
      "src": "^/flutter.js",
      "dest": "/build/web/flutter.js"
    },
    {
      "src": "^/icons/(.*)",
      "dest": "/build/web/icons/$1"
    },
    {
      "src": "^/(.*\\.[a-z0-9]+)$",
      "dest": "/build/web/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/build/web/index.html"
    }
  ]
} 